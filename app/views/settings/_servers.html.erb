<%= govuk_summary_list(card: {
  title: "GovWifi servers",
  actions: [
    govuk_link_to("How to connect to these servers",
                  "https://docs.wifi.service.gov.uk/set_up/#step-3-create-a-new-wifi-installation")
  ] }) do |summary_list|
  summary_list.with_row do |row|
    row.with_key { "London" }
    row.with_value { radius_ip_list(@london_radius_ips) }
  end
  summary_list.with_row do |row|
    row.with_key(text: "Dublin")
    row.with_value { radius_ip_list(@dublin_radius_ips) }
  end
end %>
