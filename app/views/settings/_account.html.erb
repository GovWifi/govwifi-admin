<%= govuk_summary_list(card: {title: "Your Account"}) do |summary_list|
  summary_list.with_row do |row|
    row.with_key { 'Email address' }
    row.with_value { current_user.email }

  end
  summary_list.with_row do |row|
    row.with_key(text: 'Password')
    row.with_action(text: "Change", href: edit_user_registration_path, visually_hidden_text: 'your password')
  end
end %>
